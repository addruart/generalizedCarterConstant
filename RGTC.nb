Cell[TextData[{
 "\t(v) The function  ",
 StyleBox["Grad2Norm[x__]",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " takes either 1 or 2  ",
 StyleBox["scalar",
  FontSlant->"Italic"],
 " functions as arguments and computes the square norm of the gradient of ",
 StyleBox["x",
  FontFamily->"Courier"],
 " (inner product): ",
 StyleBox["Grad2Norm[x]",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 "= ",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox["g", "ab"], 
   SubscriptBox["x", 
    RowBox[{",", "a"}]], 
   SubscriptBox["x", 
    RowBox[{",", "b"}]]}]],ExpressionUUID->
  "9241ee56-e847-46ff-be37-5c35dcacd293"],
 StyleBox[" ",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 "and  ",
 StyleBox["Grad2Norm[x,y]",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 "= ",
 Cell[BoxData[
  RowBox[{
   SuperscriptBox["g", "ab"], 
   SubscriptBox["x", 
    RowBox[{",", "a"}]], 
   SubscriptBox["y", 
    RowBox[{",", "b"}]]}]],ExpressionUUID->
  "35f782a2-d38d-431e-bc22-c09845352221"],
 "."
}], "Text",
 FontSize->16,ExpressionUUID->"f76cf30c-7332-4f2f-9f44-b9d4bedc397e"],

