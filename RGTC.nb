 Cell[BoxData[
  TagBox["",
   DisplayForm]],ExpressionUUID->"3c2e710a-4b73-4f94-b5ad-f099e3175d2e"],
 "{~1 sec, <1 MB} for ",
 StyleBox["Dim",
  FontFamily->"Courier"],
 "=6, {~20 sec, ~7 MB} for ",
 StyleBox["Dim",
  FontFamily->"Courier"],
 "=7, and {> 10 min, >120 MB} for ",
