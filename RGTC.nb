Cell[BoxData[
 RowBox[{"RGtensors", "[", 
  RowBox[{
   RowBox[{"g", "/.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{"A", "[", "t", "]"}], "->", 
       RowBox[{"1", "+", 
        RowBox[{"t", "^", "2"}]}]}], ",", 
      RowBox[{
       RowBox[{"B", "[", "t", "]"}], "->", 
       RowBox[{"1", "+", 
        RowBox[{"t", "^", "2"}]}]}], ",", 
      RowBox[{
       RowBox[{"C", "[", "t", "]"}], "->", 
       RowBox[{"4", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"2", "a", " ", "t"}], "-", 
           RowBox[{"t", "^", "2"}]}], ")"}], "/", 
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"t", "^", "2"}]}], ")"}]}]}]}], ",", 
      RowBox[{
       RowBox[{"F", "[", "t", "]"}], "->", 
       RowBox[{
