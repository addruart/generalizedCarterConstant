           RowBox[{"1", ",", "1"}], "]"}], "]"}], "\[Equal]", "1"}], ",", 
        RowBox[{"Delete", "[", 
         RowBox[{"factors", ",", "1"}], "]"}], ",", "factors"}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"tableToSum", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Product", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"l", "==", "k"}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"factors", "[", 
              RowBox[{"[", 
               RowBox[{"l", ",", "2"}], "]"}], "]"}], 
             RowBox[{
              RowBox[{"factors", "[", 
               RowBox[{"[", 
                RowBox[{"l", ",", "1"}], "]"}], "]"}], "^", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"factors", "[", 
                 RowBox[{"[", 
                  RowBox[{"l", ",", "2"}], "]"}], "]"}], "-", "1"}], ")"}]}], 
             " ", 
             RowBox[{"ToString", "[", 
              RowBox[{"\"\<D\>\"", "<>", 
               RowBox[{"ToString", "[", 
                RowBox[{"factors", "[", 
                 RowBox[{"[", 
                  RowBox[{"l", ",", "1"}], "]"}], "]"}], "]"}]}], "]"}]}], 
            ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"factors", "[", 
              RowBox[{"[", 
               RowBox[{"l", ",", "1"}], "]"}], "]"}], "^", 
             RowBox[{"factors", "[", 
              RowBox[{"[", 
               RowBox[{"l", ",", "2"}], "]"}], "]"}]}]}], 
           "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", ",", 
          RowBox[{"{", 
           RowBox[{"l", ",", "1", ",", 
            RowBox[{"Length", "[", "factors", "]"}]}], "}"}]}], 
         "\[IndentingNewLine]", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", 
          RowBox[{"Length", "[", "factors", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Sum", "[", 
